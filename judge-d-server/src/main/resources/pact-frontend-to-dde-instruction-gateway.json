{
  "provider": {
    "name": "instruction-gateway"
  },
  "consumer": {
    "name": "frontend"
  },
  "interactions": [
    {
      "description": "a request for instruction details",
      "request": {
        "method": "GET",
        "path": "/instructions/123456789012345678"
      },
      "response": {
        "status": 200,
        "body": {
          "amount": {
